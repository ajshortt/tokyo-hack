  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#495495",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string.quoted",
        "string.template",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#fa9818" //fa9818
      }
    },
    {
      "name": "Punctuation within templates",
      "scope": "string.template meta.embedded.line",
      "settings": {
        "foreground": "#b6b1b1"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "entity.name.variable"],
      "settings": {
        "foreground": "#f08df0"
      }
    },
    {
      "name": "Language variable",
      "scope": "variable.language",
      "settings": {
        "foreground": "#fa6771",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Parameter",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage (declaration or modifier keyword)",
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#FED243"
      }
    },
    {
      "name": "Constant",
      "scope": "constant",
      "settings": {
        "foreground": "#E84B3C"
      }
    },
    {
      "name": "Regex",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#E84B3C"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#E84B3C"
      }
    },
    {
      "name": "Language constant (boolean, null)",
      "scope": "constant.language",
      "settings": {
        "foreground": "#E84B3C"
      }
    },
    {
      "name": "Character escape",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#00BBFF" // 36f9f6
      }
    },
    {
      "name": "Entity",
      "scope": "entity.name",
      "settings": {
        "foreground": "#fa6771"
      }
    },
    {
      "name": "HTML or XML tag",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#26FF8F"
      }
    },
    {
      "name": "HTML or XML tag brackets",
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#00BBFF" // 36f9f6
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#FED243"
      }
    },
    {
      "name": "Tag attribute HTML",
      "scope": "entity.other.attribute-name.html",
      "settings": {
        "foreground": "#FED243",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.type", "meta.attribute.class.html"],
      "settings": {
        "foreground": "#fa6771" //fa6771
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#D50"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "variable.function"],
      "settings": {
        "foreground": "#00BBFF" // 36f9f6
      }
    },
    {
      "name": "JS Export",
      "scope": ["keyword.control.export.js", "keyword.control.import.js"],
      "settings": {
        "foreground": "#26FF8F"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#FED243"
      }
    },
    {
      "name": "Control keyword",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#FED243"
      }
    },
    {
      "name": "Operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#FED243"
      }
    },
    {
      "name": "Special operator",
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#FED243"
      }
    },
    {
      "name": "Unit",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#E84B3C"
      }
    },
    {
      "name": "Support",
      "scope": "support",
      "settings": {
        "foreground": "#fa6771"
      }
    },
    {
      "name": "Support function",
      "scope": "support.function",
      "settings": {
        "foreground": "#36f9f6" // 36f9f6
      }
    },
    {
      "name": "Support variable",
      "scope": "support.variable",
      "settings": {
        "foreground": "#f08df0"
      }
    },
    {
      "name": "Object literal key / property",
      "scope": ["meta.object-literal.key", "support.type.property-name"],
      "settings": {
        "foreground": "#f08df0"
      }
    },
    {
      "name": "Key-value separator",
      "scope": "punctuation.separator.key-value",
      "settings": {
        "foreground": "#b6b1b1"
      }
    },
    {
      "name": "Embedded puncuation",
      "scope": "punctuation.section.embedded",
      "settings": {
        "foreground": "#FED243"
      }
    },
    {
      "name": "Template expression",
      "scope": [
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end"
      ],
      "settings": {
        "foreground": "#26FF8F"
      }
    },
    {
      "name": "CSS property",
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#26FF8F"
      }
    },
    {
      "name": "JS Switch control",
      "scope": "switch-block.expr.js",
      "settings": {
        "foreground": "#26FF8F"
      }
    },
    {
      "name": "JS object path",
      "scope": "variable.other.constant.property.js, variable.other.property.js",
      "settings": {
        "foreground": "#1DA0CF"
      },
    },
    {
      "name": "Color",
      "scope": "constant.other.color",
      "settings": {
        "foreground": "#E84B3C"
      }
    },
    {
      "name": "Font names",
      "scope": "support.constant.font-name",
      "settings": {
        "foreground": "#E84B3C"
      }
    },
    {
      "name": "CSS #id",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#00BBFF" // 36f9f6
      }
    },
    {
      "name": "Pseudo CSS",
      "scope": [
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "#D50"
      }
    },
    {
      "name": "CSS support functions (rgb)",
      "scope": "support.function.misc.css",
      "settings": {
        "foreground": "#fa6771"
      }
    },
    {
      "name": "Markup heading",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#f08df0"
      }
    },
    {
      "name": "Markup text",
      "scope": ["text.html", "keyword.operator.assignment"],
      "settings": {
        "foreground": "#ffffffee"
      }
    },
    {
      "name": "Markup quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#b6b1b1cc",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup list",
      "scope": "beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#f08df0"
      }
    },
    {
      "name": "Markup link",
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#D50"
      }
    },
    {
      "name": "Markup link description",
      "scope": "string.other.link.description",
      "settings": {
        "foreground": "#E84B3C"
      }
    },
    {
      "name": "Python function call",
      "scope": "meta.function-call.generic.python",
      "settings": {
        "foreground": "#00BBFF" // 36f9f6
      }
    },
    {
      "name": "C# storage type",
      "scope": "storage.type.cs",
      "settings": {
        "foreground": "#fa6771"
      }
    },
    {
      "name": "C# local variable",
      "scope": "entity.name.variable.local.cs",
      "settings": {
        "foreground": "#f08df0"
      }
    },
    {
      "name": "C# properties and fields",
      "scope": [
        "entity.name.variable.field.cs",
        "entity.name.variable.property.cs"
      ],
      "settings": {
        "foreground": "#f08df0"
      }
    },
    {
      "name": "C++ operators",
      "scope": "source.cpp keyword.operator",
      "settings": {
        "foreground": "#FED243"
      }
    },
    {
      "name": "Elixir Classes",
      "scope": [
        "source.elixir support.type.elixir",
        "source.elixir meta.module.elixir entity.name.class.elixir"
      ],
      "settings": {
        "foreground": "#00BBFF" // 36f9f6
      }
    },
    {
      "name": "Elixir Functions",
      "scope": "source.elixir entity.name.function",
      "settings": {
        "foreground": "#26FF8F"
      }
    },
    {
      "name": "Elixir Constants",
      "scope": [
        "source.elixir constant.other.symbol.elixir",
